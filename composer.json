{
    "name": "hautelook/gearman-bundle",
    "description": "Symfony2 Bundle that provides a service to submit Gearman jobs",
    "keywords": ["Symfony2", "Gearman"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Baldur Rensch",
            "email": "brensch@gmail.com"
        },
        {
            "name": "Brandon Woodmansee",
            "email": "woodmansee@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/framework-bundle": "~2.1",
        "symfony/event-dispatcher": "~2.1",
        "symfony/console": "~2.1",
        "necromant2005/gearman-stats": "1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "symfony/browser-kit": "~2.1",
        "symfony/form": "~2.1",
        "symfony/validator": "~2.1",
        "symfony/yaml": "~2.1",
        "symfony/class-loader": "~2.1",
        "liip/monitor": "~1.0"
    },
    "suggest": {
        "ext-gearman": "The PECL Gearman extension",
        "liip/monitor": "To enable the gearman monitor"
    },
    "autoload": {
        "psr-0": { "Hautelook\\GearmanBundle": "" }
    },
    "target-dir": "Hautelook/GearmanBundle"
}
